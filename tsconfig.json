{
    "compileOnSave": false,
    "compilerOptions": {
        "strict": true,
        "lib": [
            "dom",
            "es2015",
            "es2016",
            "es2017",
            "es2018"
        ],
        "jsx": "react",
        "target": "es6",
        "module": "es6",
        "moduleResolution": "node",
        "removeComments": true,
        "preserveConstEnums": true,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules"
    ]
}
